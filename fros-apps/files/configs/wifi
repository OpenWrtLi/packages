config wifi-device
	option band '2g'
	option htmode 'HT20'
	option country 'US'
	option name '2.4G'
	option channel '6'

config wifi-device
	option band '5g'
	option htmode 'VHT80'
	option country 'US'
	option name '5G'
	option channel '36'

config wifi-iface
	option radio '0'
	option key ''
	option encry_type 'TKIPAES'
	option ssid 'fros_2g'
	option auth_mode 'OPEN'

config wifi-iface
	option radio '1'
	option encry_type 'TKIPAES'
	option key ''
	option auth_mode 'OPEN'
	option ssid 'fros_5g'